algoritmo "Enquanto número menor que 10 faca"
// Função :
// Autor :
// Data : 19/12/2024
// Seção de Declarações 
var

  numero : inteiro

inicio

      numero <- 1
      
      //Vai imprimir o número 9 vezes
      Enquanto numero < 10 Faca
      
               Escreval (numero)
               numero <- numero + 1
               
      FimEnquanto

fimalgoritmo
